<%- include header%>
<div class="post">
<% posts.forEach(function (post, index) { %>
<p><h2><a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2></p>
<p>
    作者：<a href="/u/<%= post.name %>"><%= post.name %></a> |
    日期：<%= post.time.minute %>
</p>
<p><%- post.post%></p>
 <span><a class="edit" href="/edit/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><button class="btn btn-success">编辑</button></a></span>
 <span><a class="edit" href="/remove/<%= post.name %>/<%= post.time.day %>/<%= post.title %>"><button class="btn btn-success">删除</button></a></span>
<% }) %>
</div>
<%- include footer%>